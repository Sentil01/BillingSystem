<div class="nested-fields">
  <%= f.hidden_field :_destroy %>

  <div>
    <%= f.label :product_id %>
    <%= f.select :product_id,@shop.products.collect{|p| [p.id]},include_blank: true %>
  </div>
  <div>

    <%= f.label :quantity %>
    <%= f.text_field :quantity %><br>

  </div>
  <div>

    <%= link_to  "Delete","#",class:"remove_fields" %>
  </div>

</div>